<% include header %>
<ul class="archive">
    <% var lastDay = 0 %>
    <% docs.forEach(function(doc){ %>
    <% if(lastDay != doc.time.split(' ')[0]){ %>
    <li><h3><%= doc.time.split(' ')[0] %></h3></li>
    <% lastDay = doc.time.split(' ')[0] } %>
    <li><time><%= doc.time.split(' ')[0] %></time></li>
    <li><a href="/u/<%= doc.name %>/<%= doc.time %>/<%= doc.title %>"><%= doc.title %></a></li>
    <%})%>
</ul>
<% include footer %>